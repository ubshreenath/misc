    <script type="text/javascript">
    function saveContact() {
        var firstName = document.getElementById('loginForm:txtFirstName').value;
        var lastName = document.getElementById('loginForm:txtLastName').value;
        var phone = document.getElementById('loginForm:txtContactNo').value;
        var email = document.getElementById('loginForm:txtEmail').value;
        var degree = document.getElementById('loginForm:selectDegree').value;
        var year = document.getElementById('loginForm:selectGraduationYear').value;
        var month = document.getElementById('loginForm:selectGraduationMonth').value;
		console.log('degree'+degree);
        Visualforce.remoting.Manager.invokeAction(
            '{!$RemoteAction.ASUCOntroller.saveContact}',
            firstName, lastName, email, phone,  degree, year, month,
            function(result, event){
                if (event.status) {
                    console.log('Success');
                    $("form#loginForm").slideUp("slow");
                    $("#thankyou").hide().removeClass("hidden").fadeIn();
                    $(document).scrollTop(0);
                } else if (event.type === 'exception') {
                    $("form#loginForm").slideUp("slow");
                    $("#error").hide().removeClass("hidden").fadeIn();
                    $(document).scrollTop(0);
                } else {
                    $("form#loginForm").slideUp("slow");
                    $("#error").hide().removeClass("hidden").fadeIn();
                    $(document).scrollTop(0);
                }
            }, 
            {escape: true}
        );
    }
